<h1>Sign in into existing account</h1>

<form action="<%= sessions_url %>" method="POST">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label for="sigin-email">Email</label>
  <input id="sigin-email", type="text" name="user[email]" value="">

  <br><br>

  <label for="signin-pw">Password</label>
  <input id="signin-pw" type="password" name="user[password]" value="">

  <br><br>

  <input type="submit" value="Sign in">
</form>
