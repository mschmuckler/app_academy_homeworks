<h1>Make a new account</h1>

<form action="<%= users_url %>" method="POST">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  
  <label for="user-email">Username</label>
  <input id="user-email" type="text" name="user[email]" value="">

  <br><br>

  <label for="user-password">Password</label>
  <input id="user-password" type="password" name="user[password]" value="">

  <br><br>

  <input type="submit" name="" value="Sign up">
</form>
